{
  "expo": {
    "name": "miniworld",
    "slug": "locshare",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "com.ulirocks.miniworld",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ulirocks.miniworld",
      "buildNumber": "8",
      "config": {
        "googleMapsApiKey": "AIzaSyDr6Lm5DiuTBPuCcKptyq8udlKsjCF0Y2Q"
      },
      "requireFullScreen": true
    },
    "android": {
      "versionCode": 7,
      "package": "com.ulirocks.miniworld",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDr6Lm5DiuTBPuCcKptyq8udlKsjCF0Y2Q"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "725ed200-a849-4562-8b9c-b64da0b4a806"
      }
    },
    "owner": "ulirocks",
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Turning on location services allows us to show you when friends are nearby.",
          "isIosBackgroundLocationEnabled": true,
          "isAndroidBackgroundLocationEnabled": true,
          "isAndroidForegroundServiceEnabled": true
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrienation": "DEFAULT"
        }
      ]
    ]
  }
}